<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format2.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="2">
  <name>vadd_publisher</name>
  <version>0.3.0</version>
  <description>A a trivial vector-add ROS 2 publisher. Adds
  two inputs to a vector in a loop and publishes on the go at
  10 Hz.
  </description>
  <maintainer email="victorma@xilinx.com">Víctor Mayoral Vilches</maintainer>
  <license>Apache License 2.0</license>
  <author email="victorma@xilinx.com">Víctor Mayoral Vilches</author>

  <buildtool_depend>ament_cmake</buildtool_depend>  

  <depend>rclcpp</depend>
  <depend>std_msgs</depend>  
  <depend>ament_vitis</depend>  

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
